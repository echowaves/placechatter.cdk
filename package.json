{
  "name": "placechatter.cdk",
  "version": "0.1.0",
  "author": "Echowaves Corp.",
  "license": "MIT",
  "bin": {
    "placechatter.cdk": "bin/placechatter.cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "./node_modules/nyc/bin/nyc.js --reporter=html ./node_modules/mocha/bin/mocha --exit --require @babel/register ./tests/*.js --timeout 30000",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "build-lambdas": "pushd lambda-fns; rm lambdas.zip; yarn build; zip -q -r lambdas.zip *; popd",
    "test:deploy": "DEPLOY_ENV=test yarn build && yarn build-lambdas && DEPLOY_ENV=test npx cdk deploy -O cdk-exports.json",
    "test:destroy": "DEPLOY_ENV=test cdk destroy test-PlaceChatterCdkStack",
    "prod:deploy": "DEPLOY_ENV=prod yarn build && yarn build-lambdas && DEPLOY_ENV=prod npx cdk deploy -O cdk-exports.json",
    "prod:destroy": "DEPLOY_ENV=prod cdk destroy prod-PlaceChatterCdkStack",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.51.1",
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "@types/node": "18.11.9",
    "aws-cdk": "2.51.1",
    "chai": "4.3.7",
    "chai-http": "4.3.0",
    "dotenv": "16.0.3",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "lambda-log": "3.1.0",
    "mocha": "10.1.0",
    "nyc": "15.1.0",
    "pg": "8.8.0",
    "prettier": "2.7.1",
    "regenerator-runtime": "0.13.11",
    "sequelize": "6.25.7",
    "sequelize-cli": "6.5.2",
    "supertest": "6.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "1.181.0",
    "@aws-cdk/aws-ec2": "1.181.0",
    "@aws-cdk/aws-events-targets": "1.181.0",
    "@aws-cdk/aws-iam": "1.181.0",
    "@aws-cdk/aws-lambda": "1.181.0",
    "@aws-cdk/aws-rds": "1.181.0",
    "@aws-cdk/aws-s3-notifications": "1.181.0",
    "@aws-cdk/aws-secretsmanager": "1.181.0",
    "@aws-cdk/aws-sns": "1.181.0",
    "@aws-cdk/core": "1.181.0",
    "@aws-cdk/custom-resources": "1.181.0",
    "dayjs": "1.11.6",
    "global": "4.4.0",
    "node-fetch": "3.3.0",
    "secure-random-string": "1.1.3",
    "serverless-postgres": "2.0.0",
    "source-map-support": "0.5.21",
    "uuid": "9.0.0"
  }
}
